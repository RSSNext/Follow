    "react-hook-form": "7.51.4",
    "react-router-dom": "6.23.1",
    "rehype-infer-description-meta": "2.0.0",
    "rehype-parse": "9.0.0",
    "rehype-sanitize": "6.0.0",
    "rehype-stringify": "10.0.0",
    "tailwind-merge": "2.3.0",
