    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "electron": "^30.0.9",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.2.0",
    "eslint": "^9.3.0",
    "eslint-config-hyoban": "3.0.0-beta.7",
    "lint-staged": "15.2.5",
    "postcss": "8.4.38",
