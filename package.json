    "eslint-plugin-react": "^7.33.2",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "^3.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
