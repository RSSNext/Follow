{
  "name": "@follow/shared",
  "type": "module",
  "private": true,
  "author": "Follow Team",
  "license": "GPL-3.0-only",
  "homepage": "https://github.com/RSSNext",
  "repository": {
    "url": "https://github.com/RSSNext/follow",
    "type": "git"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "require": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "require": "./src/*.ts",
      "import": "./src/*.ts"
    },
    "./interface/*": {
      "types": "./src/interface/*.ts",
      "require": "./src/interface/*.ts",
      "import": "./src/interface/*.ts"
    }
  },
  "main": "./exports.ts",
  "scripts": {
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@hono/node-server": "1.13.7",
    "@t3-oss/env-core": "^0.11.1",
    "better-auth": "1.1.9-beta.1",
    "drizzle-orm": "0.37.0",
    "hono": "4.6.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sonner": "1.7.1",
    "zod": "3.24.1"
  }
}
